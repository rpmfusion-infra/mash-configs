# mash config file

[30-updates-testing]
rpm_path = %(arch)s/
repodata_path = %(arch)s/
source_path = SRPMS/
debuginfo = True
multilib = True
multilib_method = file
multilib_file  = /etc/mash/rpmfusion-free.file
tag = f30-free-updates-testing
inherit = False
strict_keys = True
keys = c0aeda6e
repoviewurl = http://download1.rpmfusion.org/free/fedora/updates/testing/30/%(arch)s/
